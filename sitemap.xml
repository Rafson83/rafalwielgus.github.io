---
layout: null
permalink: /sitemap.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url><loc>{{ site.url }}{{ site.baseurl }}/</loc></url>
  <url><loc>{{ site.url }}{{ site.baseurl }}/about/</loc></url>
  <url><loc>{{ site.url }}{{ site.baseurl }}/categories/</loc></url>
  <url><loc>{{ site.url }}{{ site.baseurl }}/contact/</loc></url>
  {% for post in site.posts %}
  <url><loc>{{ site.url }}{{ site.baseurl }}{{ post.url }}</loc></url>
  {% endfor %}
</urlset>